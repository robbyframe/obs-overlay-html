            <path fill="#CC9200" d="M35.004 6.129c1.613.379 3 1.674 3 3.914V38H40V10.043c0-3.15-2.727-4.444-4.996-3.914zM27 2.115c1.617.379 3.002 1.674 3.002 3.916V30H32V6.031c0-3.152-2.727-4.446-5-3.916zm-8 2.003c1.617.377 3.002 1.671 3.002 3.912V30H24V8.03c0-3.15-2.727-4.446-5-3.912zm-7.998 7.999c1.615.378 3.002 1.672 3.002 3.914V32H16V16.031c0-3.15-2.727-4.447-4.998-3.914z"></path>
