  <div class="emoji-container">
    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" class="lazy-svg-icon__icon lazy-icon-nvf/270b" viewBox="0 0 64 64"><path fill="#FDCA47" d="M51.395 32c-7.891 0-7.449 6-11.395 6v-4H8v10c0 9.188 3.947 18 15.779 18 19.725 0 17.752-18 31.561-26 1.711-.992-.008-4-3.945-4z"></path><path fill="#CC9200" d="M51.395 32c-.473 0-.91.023-1.326.064 3.436.33 4.877 3.004 3.271 3.936-13.508 7.826-11.928 25.213-30.316 25.964.255.009.494.036.757.036 19.723 0 17.75-18 31.559-26 1.711-.992-.008-4-3.945-4z"></path><path fill="#FDCA47" d="M32 10.043V38h8V10.043c0-5.391-8-5.391-8 0zm-8-3.998V38h8V6.045c0-5.393-8-5.393-8 0zm-8 1.999V38h8V8.044c0-5.391-8-5.391-8 0zm-8 8.001V40h8V16.045c0-5.393-8-5.393-8 0z"></path><path fill="#CC9200" d="M35.004 6.129c1.613.379 3 1.674 3 3.914V38H40V10.043c0-3.15-2.727-4.444-4.996-3.914zM27 2.115c1.617.379 3.002 1.674 3.002 3.916V30H32V6.031c0-3.152-2.727-4.446-5-3.916zm-8 2.003c1.617.377 3.002 1.671 3.002 3.912V30H24V8.03c0-3.15-2.727-4.446-5-3.912zm-7.998 7.999c1.615.378 3.002 1.672 3.002 3.914V32H16V16.031c0-3.15-2.727-4.447-4.998-3.914z"></path><path fill="#CC9200" d="M40 36h-2c-3.824 1.178-10 6-10 13.675C30.043 42.814 36 38 40 38v-2zm-22.5-4s-2 0-5.5 2c0 0 3-2.5 3-6 .5 4 2.5 4 2.5 4zm8-.5s-3-.5-5.5.5c0 0 2.5-1.5 3-5.5.5 4 2.5 5 2.5 5zm8.5.5s-2-1.5-5.5-.5c0 0 2.5-1.5 2.5-5.5 0 4 3 6 3 6z"></path></svg>
  </div>
</div>

<script>

document.addEventListener('DOMContentLoaded', () => {
    // Function to load translations from a JSON file
    async function loadTranslations() {
        try {
            const response = await fetch('/assets/translations.json');
            if (!response.ok) {
                throw new Error(`HTTP error! Status: ${response.status}`);
            }
            const translations = await response.json();
            
            // Extract the language code from the URL hash
            let lang = 'en'; // Default language
            const supportedLanguages = Object.keys(translations); // ['en', 'pt', 'fr', 'ru', 'ja', 'br', ...]
    
            // Get the language code from the hash, stripping the '#' symbol
            const hashLang = window.location.hash.substring(1).toLowerCase();
            if (supportedLanguages.includes(hashLang)) {
                lang = hashLang;
            }
    
            // Check if translations for the selected language exist
            if (!translations[lang]) {
                console.warn(`No translations found for language: ${lang}`);
                return;
            }
            const languageData = translations[lang];
    
            // Update the Reactions button label using the "reactions_button" key
            document.querySelectorAll('.footer-button-base__button-label').forEach(el => {
                if (el.textContent.trim() === 'Reactions') {
                    el.textContent = languageData.reactions_button || el.textContent;
                }
            });
    
            // Update the Raise Hand button label using the "raise_hands" key
            document.querySelectorAll('.reaction-simple-picker__content-wrapper').forEach(el => {
                if (el.textContent.trim() === 'Raise Hand') {
                    el.textContent = languageData.raise_hands || el.textContent;
                }
            });
    
        } catch (error) {
            console.error('Error loading translations:', error);
        }
    }
    
    // Run the function to load and apply translations
    loadTranslations();
    
    // Re-apply translations when the hash changes
    window.addEventListener('hashchange', loadTranslations);
});

    function togglePicker() {
        const picker = document.getElementById('reactionPicker');
        const buttonWrapper = document.getElementById('buttonWrapper');

        picker.style.opacity = '1';
    }

    // Hide the picker when clicking outside of it
    document.addEventListener('click', function (event) {
        const picker = document.getElementById('reactionPicker');
        const buttonWrapper = document.getElementById('buttonWrapper');
        if (!picker.contains(event.target) && !buttonWrapper.contains(event.target)) {
            picker.style.opacity = '0';
        }
    });

    // Start the cursor at the bottom center of the screen
    const cursor = document.querySelector(".simulated-cursor");
    cursor.style.transform = `translate(${window.innerWidth / 2}px, ${window.innerHeight - 30}px)`;

    // Simulate click on the buttonWrapper button
    const buttonWrapper = document.getElementById('buttonWrapper');
    const buttonRect = buttonWrapper.getBoundingClientRect();

    setTimeout(() => {
        // Move cursor to the buttonWrapper
        cursor.style.transform = `translate(${buttonRect.left + window.pageXOffset + 45}px, ${buttonRect.top + window.pageYOffset + 15}px)`;

        // Simulate click
        setTimeout(() => {
          document.querySelector('.button-wrapper').classList.add('active');
          setTimeout(() => {
            buttonWrapper.click();
          }, 300)
        }, 1000)

        // Remove the 'active' class after a short delay
        setTimeout(() => {
            buttonWrapper.classList.remove('active');

            // Proceed with the animation inside the setTimeout 3000 block
            setTimeout(() => {
                const elements = document.querySelectorAll(".reaction-simple-picker__block");

              const element = elements[7] // raise hands;
              const rect = element.getBoundingClientRect();

              // Simulate the cursor moving
              cursor.style.transform = `translate(${rect.left + window.pageXOffset + 185}px, ${rect.top + window.pageYOffset + 15}px)`;

              setTimeout(() => {
                // Add the hover class to the current element
                element.classList.add('hovered');
                element.querySelector('.reaction-simple-picker__tooltip-wrapper').classList.add('hovered');
              }, 1000)

              setTimeout(() => {
                document.querySelectorAll('button.picker')[7].classList.add('active')
                setTimeout(() => {
                  const picker = document.getElementById('reactionPicker');
                  picker.style.opacity = '0';
                  document.querySelector('.nonverbal-feedback-container--emoji-only').style.opacity = '1'
                  setTimeout(() => {
                    cursor.style.transform = `translate(${window.innerWidth / 2}px, ${window.innerHeight - 30}px)`;
                  }, 200)
                }, 200)
              }, 4000)
            }, 1500);

        }, 200); // Delay for removing 'active' class

    }, 2000); // Delay for the initial move and click on the buttonWrapper
</script>



</body></html>
